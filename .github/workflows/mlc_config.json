{
    "ignorePatterns": [
      {
        "pattern": "^http://localhost"
      }
    ],
    "replacementPatterns": [
      {
        "pattern": "^/",
        "replacement": "{{BASEURL}}/"
      }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [200, 206]
  }